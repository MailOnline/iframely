<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <style>
        html, body, object, embed, iframe {
            margin:  0;
            padding: 0;
            height: 100%;
            width: 100%;
            background: white;
            color: white;
        }
        body {
            position: relative;
        }
    </style>
    <script type="text/javascript" src="<%= CONFIG.baseStaticUrl %>/js/iframely-utils.js"></script>
    <script>

        function init() {

            var d = document.getElementById('iframely-content');

            $.iframely.registerHeightGetter(function() {
                return d.scrollHeight;
            });
        }

    </script>
<body onload="init();">
    <div id='iframely-content'>
        <%- html %>
    <div>
</body>
</html>